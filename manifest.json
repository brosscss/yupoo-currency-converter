{
  "manifest_version": 3,
  "name": "Yupoo Currencies Converter",
  "version": "1.4",
  "permissions": ["storage", "tabs"],
  "content_scripts": [
    {
      "matches": ["*://*.x.yupoo.com/*", "*://*.x.zhidian-inc.cn/*"],
      "js": ["popup.js", "content.js"]
    }
  ],
  "background": {
    "service_worker": "runtime.js"
  },
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
